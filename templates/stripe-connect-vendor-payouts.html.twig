<div class="stripe-connect-vendor-payouts">
    <h1>Payouts for {{ vendor.name }}</h1>
    
    <table>
      <thead>
        <tr>
          <th>Date</th>
          <th>Amount</th>
          <th>Currency</th>
          <th>Status</th>
          <th>Method</th>
        </tr>
      </thead>
      <tbody>
        {% for payout in payouts %}
          <tr>
            <td>{{ payout.created|date('Y-m-d H:i:s') }}</td>
            <td>{{ payout.amount / 100 }}</td>
            <td>{{ payout.currency|upper }}</td>
            <td>{{ payout.status }}</td>
            <td>{{ payout.method }}</td>
          </tr>
        {% endfor %}
      </tbody>
    </table>
  </div>